<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SBSEDV\Bundle\TwigBundle\Twig\Extension\AssetExtension">
            <tag name="twig.extension" />
        </service>
        <service id="SBSEDV\Bundle\TwigBundle\Twig\Extension\CallStaticExtension">
            <tag name="twig.extension" />
        </service>
        <service id="SBSEDV\Bundle\TwigBundle\Twig\Extension\CookieConfigExtension">
            <argument key="$config" />
            <tag name="twig.extension" />
        </service>
        <service id="SBSEDV\Bundle\TwigBundle\Twig\Extension\ParameterExtension">
            <argument type="service" key="$parameters" id="Symfony\Component\DependencyInjection\ParameterBag\ParameterBagInterface" />
            <tag name="twig.extension" />
        </service>
    </services>
</container>
